   "source": [
    "def add_ing_ly(string):\n",
    "    if len(string) < 3:\n",
    "        return string\n",
    "\n",
    "    if string[-3:] == 'ing':\n",
    "        return string + 'ly'\n",
    "    else:\n",
    "        return string + 'ing'\n",
    "\n",
    "input_string = input(\"Enter a string: \")\n",
    "\n",
    "result = add_ing_ly(input_string)\n",
    "print(\"Modified string:\")\n",
    "print(result)"
   ]
  }
