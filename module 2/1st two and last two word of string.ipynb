    "def get_string_from_chars(string):\n",
    "    if len(string) < 2:\n",
    "        return \"\"\n",
    "\n",
    "    return string[:2] + string[-2:]\n",
    "\n",
    "# Test the function\n",
    "input_string = input(\"Enter a string: \")\n",
    "\n",
    "result = get_string_from_chars(input_string)\n",
    "print(\"Constructed string:\")\n",
    "print(result)"
   ]
