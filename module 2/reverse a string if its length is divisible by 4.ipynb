    "def reverse_string_if_multiple_of_4(string):\n",
    "    if len(string) % 4 == 0:\n",
    "        return string[::-1]\n",
    "    else:\n",
    "        return string\n",
    "\n",
    "\n",
    "input_string = input(\"Enter a string: \")\n",
    "\n",
    "result = reverse_string_if_multiple_of_4(input_string)\n",
    "print(\"Modified string:\")\n",
    "print(result)"
   ]
